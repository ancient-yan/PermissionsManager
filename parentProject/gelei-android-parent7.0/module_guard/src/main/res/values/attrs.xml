<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="TimeSegmentTableView" tools:ignore="ResourceName">
        <!--行数-->
        <attr name="tstv_row" format="integer"/>
        <!--一行的高度-->
        <attr name="tstv_row_height" format="dimension|reference"/>
        <!--列数-->
        <attr name="tstv_column" format="integer"/>
        <!--一个格子代表几个数值-->
        <attr name="tstv_box_value" format="integer"/>
        <!--文字大小-->
        <attr name="tstv_text_size" format="dimension"/>
        <!--默认文字颜色-->
        <attr name="tstv_text_color" format="reference|color"/>
        <!--选中的文字颜色-->
        <attr name="tstv_text_color_selected" format="reference|color"/>
        <!--描边颜色-->
        <attr name="tstv_box_line_color" format="reference|color"/>
        <!--描边颜色-->
        <attr name="tstv_box_line_width" format="reference|dimension"/>
        <!--选择填充颜色颜色-->
        <attr name="tstv_selected_area_color" format="reference|color"/>
        <!--盒子的其实计数-->
        <attr name="tstv_box_index_start" format="integer"/>
    </declare-styleable>

    <declare-styleable name="LevelView">
        <attr name="lv_icon_normal_res" format="reference"/>
        <attr name="lv_icon_selected_res" format="reference"/>
        <attr name="lv_name" format="string"/>
        <attr name="lv_desc" format="string"/>
        <attr name="lv_checked" format="boolean"/>
        <attr name="lv_is_recommend" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="OperationTipsView">
        <attr name="opv_tips" format="string|reference"/>
    </declare-styleable>

    <declare-styleable name="MarkView">
        <attr name="mk_color" format="color" />
        <attr name="mk_corner_radius" format="dimension|reference" />
        <attr name="mk_text_color" format="color" />
        <attr name="mk_text" format="string" />
    </declare-styleable>

</resources>